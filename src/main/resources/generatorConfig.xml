<?xml version="1.0" encoding="utf-8"?>
<!--基础配置-->
<generatorConfig>

    <!-- 项目路径(service) -->
    <projectUrl>D:\myLocalProject\test-maven</projectUrl>
    <!-- controller项目路径 -->
    <controllerProjectUrl>D:\myLocalProject\test-maven</controllerProjectUrl>
    <!-- jsp项目路径 -->
    <viewPrefixUrl>\src\main\webapp\WEB-INF\jsp\</viewPrefixUrl>
    <!-- 是否生成jsp文件存放位置 -->
    <viewPrefix>claim</viewPrefix>


    <!-- 是否生成Entity文件存放位置-->
    <entityUrl>com.cn.hcw.entity</entityUrl>
    <!-- 是否生成Dao文件存放位置-->
    <daoUrl>com.cn.hcw.dao</daoUrl>
    <!-- 是否生成Service文件存放位置(impl包与service同级) -->
    <serviceUrl>com.cn.hcw.service</serviceUrl>
    <!-- 是否生成Controller文件存放位置 -->
    <controllerUrl>com.cn.hcw.Controller</controllerUrl>
    <!-- 是否生成SqlXml文件存放位置 -->
    <sqlXmlUrl>mapper</sqlXmlUrl>

    <!-- 是否要分页扩展 -->
    <paging>false</paging>

    <!-- 是否映射所有表 -->
    <generatorAllTable>false</generatorAllTable>
    <!-- 需映射的表(如果上面是false要填写表名)，多个表用英文逗分割 -->
    <needGeneratorTables>tb_student</needGeneratorTables>

    <!-- 需要继承的BaseController -->
    <baseController>com.cn.hcw.base.BaseController</baseController>
    <!-- 需要继承的BaseService -->
    <baseService></baseService>

    <!-- 是否生成Controller -->
    <createController>true</createController>

    <!-- 是否生成jsp -->
    <createView>false</createView>

    <!-- 是否生成字段描述(注释) -->
    <createFieldDescribe>true</createFieldDescribe>

    <!-- 生成的类是否去掉表名前缀 -->
    <cutTablePrefix>true</cutTablePrefix>
    <!-- 表名前缀 -->
    <tablePrefix>tb_</tablePrefix>

    <!-- 是否生成Entity -->
    <createEntity>true</createEntity>
    <!-- 是否生成createDao -->
    <createDao>true</createDao>
    <!-- 是否生成Service -->
    <createService>true</createService>
    <!-- 是否生成ServiceImpl -->
    <createServiceImpl>true</createServiceImpl>
    <!-- 是否生成XMLMapper -->
    <createXMLMapper>true</createXMLMapper>

</generatorConfig>